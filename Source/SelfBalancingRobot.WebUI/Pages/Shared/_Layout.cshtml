@inject SelfBalancingRobot.WebUI.Configuration.AppSettings settings
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="/img/apple-icon.png">
    <link rel="icon" type="image/png" href="/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <meta name="description" content="Self-Balancing Robot .NET v@(settings.Version)" />
    <meta name="author" content="Anwar Minarso" />
    <title>@ViewData["Title"]</title>
    <link href="~/js/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/js/bootstrap-toggle/css/bootstrap4-toggle.min.css" rel="stylesheet" />
    <link href="~/css/light-bootstrap-dashboard.min.css" rel="stylesheet" />
    <link href="~/css/site.min.css" rel="stylesheet" />
    <link href="~/css/fontawesome.min.css" rel="stylesheet" />
    <link href="~/css/fa-brands.min.css" rel="stylesheet" />
    <link href="~/css/nextgenicons.min.css" rel="stylesheet" />
    @await RenderSectionAsync("Head", required: false)
</head>
@*<body class="sidebar-mini">*@
<body>
    <div class="wrapper">
        <vc:side-bar />
        
        <div class="main-panel" id="main-panel">
            @await RenderSectionAsync("FixedPlugin", required: false)
            <partial name="_NavBar" />
            <!-- Main Content -->
            <div class="content">
                <div class="container-fluid" id="main-Container">
                    @RenderBody()
                </div>
            </div>
            <!-- End of Main Content -->
            <partial name="_Footer" />
        </div>
    </div>
    <script src="~/js/jquery/jquery.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/bootstrap-toggle/js/bootstrap4-toggle.min.js"></script>
    <script src="~/js/bootstrap/bootstrap-switch.js"></script>
    <script src="~/js/bootstrap/bootstrap-notify.js"></script>
    <script src="~/js/light-bootstrap-dashboard.js"></script>
    <script src="~/js/signalr/signalr.min.js"></script>
    <script src="~/js/bootbox/bootbox.min.js"></script>
    <script src="~/js/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/a2n/a2n.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
